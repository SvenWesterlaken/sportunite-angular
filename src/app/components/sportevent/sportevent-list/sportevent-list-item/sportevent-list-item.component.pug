a([routerLink]="[index]" routerLinkActive="active")
    mat-card
        mat-card-header
            .container
                h5 {{event.name}}
                .header
                    label.text.small {{event.organisor.firstname}} {{event.organisor.lastname}}
                    img(src="data:image/jpeg;base64," alt="Avatar")

        mat-card-content
            .container
                .right
                    P AANWEZIGEN
                .left
                    P {{attendees}} / {{event.maxAttendees}}
            mat-progress-bar(mode="determinate", [value]="value", [color]="setColor()")
        mat-card-content.footer
            .container
                .left
                    // mat-icon(svgIcon="cash")
                    // label &euro;25
                    mat-icon(svgIcon="clock" )
                    label {{event.eventStartTime | amDateFormat: 'HH:mm'}}-{{event.eventEndTime | amDateFormat: 'HH:mm'}}
                .right
                    mat-icon(svgIcon="map")
                    label {{event.reservation.hall.building.name}}
                    label.small {{event.reservation.hall.building.address.streetName}} {{event.reservation.hall.building.address.houseNumber}}, {{event.reservation.hall.building.address.city}}
                    // mat-icon(svgIcon="map-marker")
                    // label 0.5 km